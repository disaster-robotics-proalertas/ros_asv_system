  <include file="$(find asv_simulator)/launch/two_obstacles.launch"/>
  <include file="$(find asv_obstacle_tracker)/launch/default.launch"/>

  <node pkg="rviz"
        type="rviz"
        name="rviz"
        args="-d $(find asv_simulator)/config/rviz/three_vessels.rviz" />


  <node pkg="rosbag"
